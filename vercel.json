{
   "version": 2,
   "builds": [
     {
       "src": "./index.js",
       "use": "@vercel/node"
     }
   ],
   "routes": [
    {
      "src": "/tetris",
      "dest": "/tetris.html"
    },
     {
       "src": "/(.*)",
       "dest": "/"
     },
     {
      "src": "/",
      "dest": "/index.html"
    }
    
   ]
 }